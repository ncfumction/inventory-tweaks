@ require trident-util:generators
@ require anonymous_resources:score

var x = anonymous_score()
set $x = 0

eval generateFunctionTree(
    resource</a>,
    int_range<0..36>,
    4,
    function (range, next) {
        if (range.min == 0)
            if entity @s function $next
        else
            unless entity @s function $next
    },
    function (i) {
        execute if entity @s noop
    }
)


define function /b {
    execute if entity @s noop
    for (var i = 1; i < 36; i++) {
        execute unless entity @s noop
    }
}

define function /1 {
    for (var i = 0; i < 1000; i++) {
        function /../a
    }
}
define function /2 {
    for (var i = 0; i < 1000; i++) {
        function /../b
    }
}
