define function /a {
    for (var i = 0; i < 500; i++) {
        for (var i = 0; i < 27; i++) {
            item replace block ~ ~ ~ ${'container.' + i} from entity @s ${'container.' + i}
        }
    }
}
define function /b {
    for (var i = 0; i < 500; i++) {
        set ${shulker_box(0)}.Items = @s.Inventory
        loot replace block ~ ~ ~ container.0 mine ${shulker_box(0)} air{drop_contents:1b}
    }
}
